{
  "name": "ebdo-api",
  "version": "0.1.0",
  "description": "API Ebdo Journal",
  "engines": {
    "npm": ">=5.5.x",
    "node": ">=8.x"
  },
  "scripts": {
    "start": "node dist/bin/server.js",
    "build":
      "rimraf dist && cross-env NODE_ENV=production babel src -d dist -s",
    "server": "babel-node dist/bin/server.js",
    "heroku-postbuild": "npm run build && npm run migrate",
    "dev": "nodemon src/bin/server.js",
    "debug-brk": "npm run debug -- --break",
    "test":
      "mocha --require test/_helpers test/**/*.spec.js --compilers js:babel-register -R dot --timeout 5000",
    "test-watch": "npm run test -- --watch",
    "lint": "eslint src/ test/",
    "lint-watch": "esw -w src/ test/",
    "cover": "cross-env NODE_ENV=test nyc npm test",
    "migrate": "sequelize db:migrate"
  },
  "author": "Steven Sans√©au",
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "@sendgrid/mail": "^6.1.4",
    "app-module-path": "^2.2.0",
    "awilix": "^2.1.0",
    "awilix-koa": "^1.0.2",
    "bristol": "^0.3.3",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.3",
    "fejl": "^1.0.2",
    "glob": "^7.1.2",
    "kcors": "2",
    "keyblade": "^0.3.2",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-respond": "^1.0.1",
    "koa-router": "^7.3.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "node-mjml-mustache-nodemailer": "^0.0.3",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemon": "^1.14.0",
    "nyc": "^11.4.1",
    "paginator": "^1.0.0",
    "palin": "^2.2.0",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "rimraf": "^2.6.2",
    "sequelize": "^4.28.6",
    "sequelize-cli": "^3.2.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "soap": "^0.23.0",
    "source-map-support": "^0.5.0",
    "sqs-producer": "^1.6.2",
    "supertest": "^3.0.0",
    "uniqid": "^4.1.1",
    "yenv": "^1.0.6"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.14.0",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0"
  },
  "directories": {
    "test": "test"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "reporter": ["text", "lcov"],
    "include": ["src/**/*.js"]
  }
}
